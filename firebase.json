{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/rest",
        "run": {
          "serviceId": "toptal-backend",
          "region": "europe-west1"
        }
      }
    ]
  }
}
